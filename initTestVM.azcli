# Run init first (Beware of last lines that deletes everything)

VM_SUBNET_NAME="subnet-vm"
VM_SUBNET_IPRANGE="10.0.2.0/24"

VM_NAME="vm-ubuntuhost"


# Create Subnet for VM
az network vnet subnet create \
    --name $VM_SUBNET_NAME \
    --vnet-name $VNET_NAME \
    --resource-group $RESOURCEGROUP \
    --address-prefixes $VM_SUBNET_IPRANGE

az vm create --name $VM_NAME --resource-group $RESOURCEGROUP --subnet $VM_SUBNET_NAME --vnet-name $VNET_NAME --image UbuntuLTS --generate-ssh-keys

